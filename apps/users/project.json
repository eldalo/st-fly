{
  "name": "users",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/users/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/users",
        "index": "apps/users/src/index.html",
        "baseHref": "/",
        "main": "apps/users/src/main.ts",
        "tsConfig": "apps/users/tsconfig.app.json",
        "assets": [],
        "styles": [],
        "scripts": [],
        "webpackConfig": "apps/users/webpack.config.ts"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/users/src/environments/environment.ts",
              "with": "apps/users/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "webpackConfig": "apps/users/webpack.config.prod.ts"
        }
      }
    },
    "serve": {
      "executor": "@nx/react:module-federation-dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "users:build",
        "hmr": true,
        "port": 3004
      },
      "configurations": {
        "development": {
          "buildTarget": "users:build:development"
        },
        "production": {
          "buildTarget": "users:build:production",
          "hmr": false
        }
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "defaultConfiguration": "production",
      "options": {
        "buildTarget": "users:build",
        "watch": false,
        "port": 3004
      },
      "configurations": {
        "development": {
          "buildTarget": "users:build:development"
        },
        "production": {
          "buildTarget": "users:build:production"
        }
      }
    }
  }
}
